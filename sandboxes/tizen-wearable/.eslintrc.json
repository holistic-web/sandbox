{
	"root": true,
	"env": {
		"node": true
	},
	"extends": [
		"plugin:vue/essential",
		"@vue/airbnb"
	],
	"rules": {
		"no-console": "off",
		"no-debugger": "off",
		"indent": [1, "tab"],
		"no-tabs": 0,
		"max-len": [1, 140],
		"comma-dangle": [2, "never"]
	},
	"parserOptions": {
		"parser": "babel-eslint"
	}
}
